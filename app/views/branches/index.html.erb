<p style="color: green"><%= notice %></p>

<h1>Branches</h1>

<div id="branches">
  <% @branches.each do |branch| %>
    <%= render branch %>
    <p>
      <%= link_to "Show this branch", school_branch_path(@school, branch) %>
    </p>
  <% end %>
</div>

<%= link_to "New branch", new_school_branch_path %>
